{
    "name": "pantheon-upstreams/upstream-configuration",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/morpht/convivial-profile.git"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "bower-asset/dropzone": "^5.5.0",
        "drupal/address": "^1.10",
        "drupal/autologout": "^1.3",
        "drupal/ayrshare": "^1.0",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/facets_pretty_paths": "^1.2",
        "drupal/field_token_value": "^2.0",
        "drupal/geofield": "^1.37",
        "drupal/geofield_map": "^2.73",
        "drupal/maxlength": "^2.0@RC",
        "drupal/moderation_dashboard": "^1.0@beta",
        "drupal/pantheon_advanced_page_cache": "^1.2",
        "drupal/profile_switcher": "^1.0@alpha",
        "drupal/responsive_preview": "^1.1",
        "drush/drush": "11.x-dev",
        "morpht/convivial-profile": "dev-master"
    },
    "extra": {
        "_README": "To make a custom upstream, clone this repository and add any dependencies to be provided by this upstream to this composer.json file. Leave the root-level composer.json file for the exclusive use of each individual site; do not modify it after your custom upstream has been published. See https://pantheon.io/docs/create-custom-upstream for more information."
    },
    "minimum-stability": "dev"
}
